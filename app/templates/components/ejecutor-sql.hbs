<div class="card">
  <div class="card-content">
    <span class="card-title">SQL</span>
    {{#if resultado}}
      <div class="row">
        {{tenpines/text-area value=resultado col="s12"}}
      </div>
    {{/if}}

    <div class="row">
      {{tenpines/text-area value=codigoSql col="s12"}}
    </div>
  </div>
  <div class="card-action left-align">
    {{tenpines/material-button label='Ejecutar' action='ejecutarStatement' }}
  </div>
</div>