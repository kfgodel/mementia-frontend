<div class="card">
  <div class="card-content">
    <span class="card-title">Borrar Relacion</span>
    <div class="row">
      {{tenpines/input-text value=parametros.idBorrable label="Id de relacion" col="s12"}}
    </div>
    {{#if mensajeDeFeedback}}
      <div class="row">
        <div class="col s12">
          <span>{{mensajeDeFeedback}}</span>
        </div>
      </div>
    {{/if}}
  </div>
  <div class="card-action left-align">
    {{tenpines/material-button label='Borrar' action='borrarRelacion' }}
  </div>
</div>