<div class="card">
  <div class="card-content">
    <span class="card-title">Crear relacion</span>
    <div class="row">
      {{tenpines/input-text value=parametros.idNodoOrigen label="Id de origen" col="s3" }}
      {{tenpines/input-text value=parametros.tipoDeRelacion label="Tipo de relacion" col="s3" }}
      {{tenpines/input-text value=parametros.idNodoDestino label="Id de destino" col="s3" }}
    </div>

    {{#if mensajeDeFeedback}}
      <div class="row">
        <div class="col s12">
          <span>{{mensajeDeFeedback}}</span>
        </div>
      </div>
    {{/if}}
  </div>
  <div class="card-action left-align">
    {{tenpines/material-button label='Crear' action='crearRelacion' }}
  </div>
</div>
