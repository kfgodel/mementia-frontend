<div class="card">
  <div class="card-content">
    <span class="card-title">Groovy</span>
    {{#if resultadoGroovy}}
      <div class="row">
        {{tenpines/text-area value=resultadoGroovy col="s12"}}
      </div>
    {{/if}}

    <div class="row">
      {{tenpines/text-area value=codigoGroovy col="s12"}}
    </div>
  </div>
  <div class="card-action left-align">
    {{tenpines/material-button label='Ejecutar' action='ejecutarGroovy' }}
  </div>
</div>